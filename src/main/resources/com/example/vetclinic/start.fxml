<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #87CEEB;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vetclinic.Start">
    <children>
        <Label layoutX="138.0" layoutY="20.0" prefHeight="38.0" prefWidth="317.0" text="Ветеринарная клиника" textFill="#fffefe">
            <font>
                <Font name="Bookman Old Style" size="26.0" />
            </font>
        </Label>
        <Label layoutX="20.0" layoutY="118.0" prefHeight="68.0" prefWidth="560.0" text="&quot;Шерстяной заклинатель&quot;" textAlignment="CENTER" textFill="#242d59">
            <font>
                <Font name="System Bold" size="42.0" />
            </font>
        </Label>
        <Button fx:id="ButtonUser" layoutX="403.0" layoutY="271.0" mnemonicParsing="false" onMouseClicked="#ClickUser" text="Клиент" textFill="#242d59">
            <font>
                <Font size="23.0" />
            </font>
        </Button>
        <Button fx:id="ButtonDoctor" layoutX="72.0" layoutY="271.0" mnemonicParsing="false" onMouseClicked="#ClickVeterinarian" prefHeight="50.0" prefWidth="157.0" text="Ветеринар" textFill="#3a4b94">
            <font>
                <Font size="23.0" />
            </font>
        </Button>
      <ImageView fitHeight="68.0" fitWidth="73.0" layoutX="518.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/white_on_trans.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
