<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #151753;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vetclinic.view.VetAppointment">
    <children>
        <Label layoutX="201.0" layoutY="14.0" prefHeight="69.0" prefWidth="198.0" text="Мои приемы" textAlignment="JUSTIFY" textFill="WHITE" wrapText="true">
            <font>
                <Font name="System Bold" size="29.0" />
            </font>
        </Label>
        <Button fx:id="back" layoutX="488.0" layoutY="350.0" mnemonicParsing="false" onMouseClicked="#toBack" style="-fx-background-color: #000000;" text="Вернуться" textFill="WHITE">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <TableView fx:id="appointmentsTable" layoutX="48.0" layoutY="83.0" prefHeight="252.0" prefWidth="503.0">
            <columns>
                <TableColumn fx:id="appointmentIdColumn" prefWidth="100.0" text="Appointment ID" />
                <TableColumn fx:id="petNameColumn" prefWidth="150.0" text="Pet Name" />
                <TableColumn fx:id="appointmentDateColumn" prefWidth="150.0" text="Date" />
                <TableColumn fx:id="appointmentTimeColumn" prefWidth="100.0" text="Time" />
            </columns>
        </TableView>


    </children>
</AnchorPane>
